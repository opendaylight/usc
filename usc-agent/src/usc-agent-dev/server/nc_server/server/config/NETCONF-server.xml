<device>
	<name>NETCONF-server</name>
	<data-models>
		<model>
			<path>/usr/local/etc/netopeer/cfgnetopeer/ietf-x509-cert-to-name.yin</path>
		</model>
		<model-main>
			<path>/usr/local/etc/netopeer/cfgnetopeer/ietf-netconf-server.yin</path>
			<feature>ssh</feature>
			<feature>inbound-ssh</feature>
			<feature>outbound-ssh</feature>
			<feature>tls</feature><feature>inbound-tls</feature><feature>outbound-tls</feature>
		</model-main>
	</data-models>
	<repo>
		<type>file</type>
		<path>/usr/local/etc/netopeer/cfgnetopeer/datastore-server.xml</path>
	</repo>
</device>
