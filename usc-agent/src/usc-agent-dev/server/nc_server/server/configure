# expand libdir
expanded_libdir=`(
	case $prefix in
		NONE) prefix=$ac_default_prefix ;;
		*) ;;
	esac
	case $exec_prefix in
		NONE) exec_prefix=$prefix ;;
		*) ;;
	esac
	eval echo $libdir
)`

