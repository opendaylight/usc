enable_dbus
enable_tls
with_stunnel
with_libnetconf
with_libdbus
with_dbus_config_dir
with_dbus_services
with_modules_dir
'
      ac_precious_vars='build_alias
host_alias
target_alias
CC
CFLAGS
LDFLAGS
LIBS
CPPFLAGS
PKG_CONFIG
PKG_CONFIG_PATH
PKG_CONFIG_LIBDIR
LIBNETCONF_CFLAGS
